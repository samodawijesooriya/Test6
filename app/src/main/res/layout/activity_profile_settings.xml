<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileSettings">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="40dp"
        android:onClick="GobackToUserProfile"
        app:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="129dp"
        android:layout_marginTop="41dp"
        android:text="Your Profile"
        android:textSize="30dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="245dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="148dp"
        android:paddingTop="10dp"
        android:textAlignment="center"
        android:background="@drawable/roundcorner"
        android:text="Add Profile Picture"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/arrow_profile"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="65dp"
        android:onClick="GoToTheAddProfilePicture"
        android:rotation="180"
        app:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/User_Name"
        android:layout_width="81dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="222dp"
        android:layout_marginEnd="271dp"
        android:text="Name"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/User_Rname"
        android:layout_width="234dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="256dp"
        android:background="@drawable/roundcorner"
        android:padding="10dp"
        android:paddingLeft="20dp"
        android:text="It's Mathee"
        android:textAlignment="center"
        android:textSize="20sp" />
    <ImageView
        android:id="@+id/arrow_Name"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="259dp"
        android:layout_marginEnd="66dp"
        android:rotation="180"
        android:onClick="GoTOChangeName"
        app:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/User_Mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="321dp"
        android:text="Mobile"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/User_Rmobile"
        android:layout_width="234dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="354dp"
        android:background="@drawable/roundcorner"
        android:padding="10dp"
        android:paddingLeft="20dp"
        android:text="0772258722"
        android:textAlignment="center"
        android:textSize="20sp" />
    <ImageView
        android:id="@+id/arrow_Mobile"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="354dp"
        android:layout_marginEnd="66dp"
        android:rotation="180"
        android:onClick="GoToChangeNumber"
        app:srcCompat="@drawable/back" />
    <TextView
        android:id="@+id/User_Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="56dp"
        android:layout_marginTop="408dp"
        android:text="Email"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/User_Remail"
        android:layout_width="234dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="443dp"
        android:background="@drawable/roundcorner"
        android:padding="10dp"
        android:paddingLeft="20dp"
        android:text="itsmathee@gmail.com"
        android:textAlignment="center"
        android:textSize="20sp" />
    <ImageView
        android:id="@+id/arrow_Email"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="443dp"
        android:layout_marginEnd="66dp"
        android:rotation="180"
        app:srcCompat="@drawable/back"
        android:onClick="GoToChangeEmail"/>
    <TextView
        android:id="@+id/User_changePassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="517dp"
        android:text="Change Password"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/User_RchangePassword"
        android:layout_width="233dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="553dp"
        android:background="@drawable/roundcorner"
        android:padding="10dp"
        android:paddingLeft="20dp"
        android:text="******"
        android:textAlignment="center"
        android:textSize="20sp" />
    <ImageView
        android:id="@+id/arrow_ChangePassword"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="553dp"
        android:layout_marginEnd="66dp"
        android:rotation="180"
        app:srcCompat="@drawable/back"
        android:onClick="GoToChnagePassword"/>
</RelativeLayout>